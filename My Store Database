

CREATE TABLE store (id INTEGER PRIMARY KEY, item TEXT, quantity INTEGER, price INTEGER, aisle INTEGER);
INSERT INTO store VALUES (1, "Shirts", 5, 3.99, 5);
INSERT INTO store VALUES (2, "Pants", 3, 10.00, 7);
INSERT INTO store VALUES (3, "Socks", 2, 2.00, 2);
INSERT INTO store VALUES (4, "Jackets", 5, 15.00, 10);
INSERT INTO store VALUES (5, "Hats", 2, 3.00, 4);
